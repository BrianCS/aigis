<html>
  <head>
    <meta charset="utf-8">
    <title>{{title}}</title>
    <style>
      body {
        margin: 0;
        padding: 0;
      }
      pre.editor {
        padding: 5px;
        overflow: scroll;
        margin: 0 5px;
      }
      .ronde-codeblock__header {
        background-color: #1d1f21;
        border-bottom: 1px solid #3c4043;
        padding: 3px 5px;
        color: #fff;
        font-size: 11px;
        text-align: left;
        margin: 5px 5px 0;
      }
      .ronde-heading {
        background-color: #555;
        color: #eee;
        padding-left: 5px;
      }
    </style>
    <link rel="stylesheet" href="/doc_assets/css/highlight/monokai.css">
    {{#each assets_css}}
    <link rel="stylesheet" href="/doc_assets/{{this}}">
    {{/each}}
    {{#each css}}
    <link rel="stylesheet" href="/doc_assets/{{this}}">
    {{/each}}
  </head>
<body>
  {{#each assets_js}}
  <script src="/doc_assets/{{this}}"></script>
  {{/each}}
  {{#if categoryName}}
  <h1 class="ronde-heading">{{categoryName}}</h1>
  {{/if}}
  {{{body}}}
</body>
</html>
