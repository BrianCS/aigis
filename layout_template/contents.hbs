{{{header}}}
  <div class="ronde-sidemenu">
  {{{sidemenu}}}
  </div>
  <div class="ronde-contents">
    <h1 class="ronde-heading">{{name}}</h1>
    
    <h2 class="ronde-module__heading">Modules</h2>
    <ul class="ronde-moduleList">
    {{#each module.list}}
      <li class="ronde-moduleList__item"><a href="#{{this}}">{{this}}</a>
    {{/each}}
    </ul>

    {{#each module.contents}}
    <div class="ronde-module">
      {{{this}}}
    </div>
    {{/each}}
  </div>
{{{footer}}}
