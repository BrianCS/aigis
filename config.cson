"name": "test_project"
# The directory containing the source files to parse recursively
"source": [
  "./css"
  # "./css2"
]
"dest": "./ronde"
# You may put doc related assets here too: images, css, js, etc.
"doc_assets": ["./doc_assets"]
# document. May also include additional folders as needed.
###
"dependencies": [
  "./dist"
]
###

###
Reference JS library from CSS file by CSON.
Write library name & file path.File path is relative to "doc_assets" path.
###
"lib":
  "jquery": "./js/lib/jquery.js"
# Markdown renderer custom template for MarkedRenderer
"md_template": "./md_template/"
# Attach class to rendererd Markdown HTML Element
# value is must be array
# "highlight": "disable"
"md_class":
  "blockquote": []
  "heading"   : []
  "hr"        : ["redline"]
  "list"      : []
  "listitem"  : []
  "paragraph" : []
  "table"     : []
  "tablerow"  : []
  "tablecell" : []
